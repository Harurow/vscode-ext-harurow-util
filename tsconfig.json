{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "lib": [
            "es2019"
        ],
        "sourceMap": true,
        "rootDir": ".",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "strict": true,
    },
    "exclude": [
        ".git/",
        ".vscode-test/",
        "node_modules/",
        "scripts/",
        "out/",
        "dist/",
        "coverage/"
    ]
}
