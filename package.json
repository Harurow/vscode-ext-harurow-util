{
  "name": "harurow-util",
  "displayName": "Harurow Util",
  "description": "自分で使うプログラマのための機能をまとめたVisual Studio Code 拡張",
  "version": "1.2.0",
  "publisher": "Harurow-the-programmer",
  "homepage": "https://github.com/Harurow/vscode.ext.harurow.util",
  "repository": {
    "type": "git",
    "url": "https://github.com/Harurow/vscode.ext.harurow.util.git"
  },
  "bugs": {
    "url": "https://github.com/Harurow/vscode.ext.harurow.util/issues"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "pascal/camel/snake/case",
    "encode/escape",
    "line-filter",
    "multiple-cursors",
    "numbering"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/copy-paste": "^1.1.30",
    "@types/encoding-japanese": "^1.0.15",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "mocha": "^3.2.0",
    "tslint": "^5.0.0",
    "typescript": "^2.2.1",
    "vscode": "^1.0.5",
    "vscode-nls": "^2.0.2"
  },
  "dependencies": {
    "copy-paste": "^1.3.0",
    "encoding-japanese": "^1.0.25"
  },
  "activationEvents": [
    "onCommand:commands.caseConverters.toPascalCase",
    "onCommand:commands.caseConverters.toCamelCase",
    "onCommand:commands.caseConverters.toUpperSnakeCase",
    "onCommand:commands.caseConverters.toLowerSnakeCase",
    "onCommand:commands.lineFilters.match",
    "onCommand:commands.lineFilters.unmatch",
    "onCommand:commands.lineFilters.contains",
    "onCommand:commands.lineFilters.notContains",
    "onCommand:commands.encodings.encodeRfc3986ShiftJis",
    "onCommand:commands.encodings.encodeRfc3986EucJp",
    "onCommand:commands.encodings.encodeRfc3986Utf8",
    "onCommand:commands.encodings.encodeRfc1866ShiftJis",
    "onCommand:commands.encodings.encodeRfc1866EucJp",
    "onCommand:commands.encodings.encodeRfc1866Utf8",
    "onCommand:commands.encodings.decodeRfc3986ShiftJis",
    "onCommand:commands.encodings.decodeRfc3986EucJp",
    "onCommand:commands.encodings.decodeRfc3986Utf8",
    "onCommand:commands.encodings.decodeRfc1866ShiftJis",
    "onCommand:commands.encodings.decodeRfc1866EucJp",
    "onCommand:commands.encodings.decodeRfc1866Utf8",
    "onCommand:commands.escapes.escapeHtml",
    "onCommand:commands.escapes.escapeHtmlAll",
    "onCommand:commands.escapes.unescapeHtml",
    "onCommand:commands.escapes.escapeUnicode",
    "onCommand:commands.escapes.escapeUnicodeAll",
    "onCommand:commands.escapes.unescapeUnicode",
    
    "onCommand:extension.esc.toUnicode",
    "onCommand:extension.esc.fromUnicode",
    "onCommand:extension.esc.fromUnicodeAll",
    "onCommand:extension.num.numbers",
    "onCommand:extension.sel.string",
    "onCommand:extension.sel.regex",
    "onCommand:extension.rep.leftsideRight",
    "onCommand:caseConverters.toPascal",
    "onCommand:caseConverters.toCamel",
    "onCommand:caseConverters.toUpperSnake",
    "onCommand:caseConverters.toLowerSnake"
  ],
  "contributes": {
    "commands": [
      {
        "command": "commands.caseConverters.toPascalCase",
        "category": "CaseConverter",
        "title": "ToPascalCase"
      },
      {
        "command": "commands.caseConverters.toCamelCase",
        "category": "CaseConverter",
        "title": "toCamelCase"
      },
      {
        "command": "commands.caseConverters.toUpperSnakeCase",
        "category": "CaseConverter",
        "title": "TO_UPPER_SNAKE_CASE"
      },
      {
        "command": "commands.caseConverters.toLowerSnakeCase",
        "category": "CaseConverter",
        "title": "to_lower_snake_case"
      },
      {
        "command": "commands.lineFilters.match",
        "category": "LineFilter",
        "title": "Remove Line If Matched Regex ..."
      },
      {
        "command": "commands.lineFilters.unmatch",
        "category": "LineFilter",
        "title": "Remove Line If Un-Matched Regex ..."
      },
      {
        "command": "commands.lineFilters.contains",
        "category": "LineFilter",
        "title": "Remove Line If Contains String ..."
      },
      {
        "command": "commands.lineFilters.notContains",
        "category": "LineFilter",
        "title": "Remove Line If NOT Contains String ..."
      },
      {
        "command": "commands.encoding.encodeRfc3986ShiftJis",
        "category": "Encode",
        "title": "To Percent Encoding (RFC3986 ' ' => %20) / Shift_JIS"
      },
      {
        "command": "commands.encoding.encodeRfc3986EucJp",
        "category": "Encode",
        "title": "To Percent Encoding (RFC3986 ' ' => %20) / EUC-JP"
      },
      {
        "command": "commands.encoding.encodeRfc3986Utf8",
        "category": "Encode",
        "title": "To Percent Encoding (RFC3986 ' ' => %20) / UTF-8"
      },
      {
        "command": "commands.encoding.encodeRfc1866ShiftJis",
        "category": "Encode",
        "title": "To Percent Encoding (RFC1866 ' ' => +) / Shift_JIS"
      },
      {
        "command": "commands.encoding.encodeRfc1866EucJp",
        "category": "Encode",
        "title": "To Percent Encoding (RFC1866 ' ' => +) / EUC-JP"
      },
      {
        "command": "commands.encoding.encodeRfc1866Utf8",
        "category": "Encode",
        "title": "To Percent Encoding (RFC1866 ' ' => +) / UTF-8"
      },
      {
        "command": "commands.encoding.deodeRfc1866ShiftJis",
        "category": "Decode",
        "title": "From Percent Encoding (RFC3986, RFC1866) / Shift_JIS"
      },
      {
        "command": "commands.encoding.deodeRfc1866EucJp",
        "category": "Decode",
        "title": "From Percent Encoding (RFC3986, RFC1866) / EUC-JP"
      },
      {
        "command": "commands.encoding.deodeRfc1866Utf8",
        "category": "Decode",
        "title": "From Percent Encoding (RFC3986, RFC1866) / UTF-8"
      },
      {
        "command": "commands.escapes.escapeHtml",
        "category": "Escape",
        "title": "To HTML Escape"
      },
      {
        "command": "commands.escapes.escapeHtmlAll",
        "category": "Escape",
        "title": "To HTML Escape All Charactors"
      },
      {
        "command": "commands.escapes.unescapeHtml",
        "category": "Unescape",
        "title": "From HTML Escape"
      },
      {
        "command": "commands.escapes.escapeUnicode",
        "category": "Escape",
        "title": "To Unicode Escape"
      },
      {
        "command": "commands.escapes.escapeUnicodeAll",
        "category": "Escape",
        "title": "To Unicode Escape All Charactors"
      },
      {
        "command": "commands.escapes.unescapeUnicode",
        "category": "Unescape",
        "title": "From Unicode Escape"
      },


      {
        "command": "extension.sel.string",
        "category": "Selection",
        "title": "Select String ..."
      },
      {
        "command": "extension.sel.regex",
        "category": "Selection",
        "title": "Select If Matched Regex ..."
      },
      {
        "command": "extension.num.numbers",
        "category": "Numbers",
        "title": "Inert Numbers to Multiple Cursors ..."
      },
      {
        "command": "extension.rep.leftsideRight",
        "category": "Replace",
        "title": "Leftside Right"
      }
    ]
  }
}
